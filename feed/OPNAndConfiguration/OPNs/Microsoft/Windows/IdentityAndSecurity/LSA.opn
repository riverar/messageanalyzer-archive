        override string ToString()
        {
            string summary = "LsarLookupPrivilegeName, ReturnValue: " + NtStatusValuesToText(ReturnValue) +
                ", Value: " + Value.ToString();
            if (Name != null)
            {
                summary += ", Name: " + Name.ToString();
            }
            return summary;
        }

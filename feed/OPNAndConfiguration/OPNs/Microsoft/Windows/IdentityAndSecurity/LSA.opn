                ", SecretName: " + SecretName.ToString() +
                ", DesiredAccess: " + EnumToString(DesiredAccess, "LSA.AccessMaskForSecretObjects");
        }
    }
    with IDL{Opnum = 28};
    
    accepts operation LsarSetSecret
